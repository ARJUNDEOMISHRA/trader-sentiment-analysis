   "cell_type": "markdown",
   "id": "8db21112-d1aa-4548-a92c-d16bbd84f529",
   "metadata": {},
   "source": [
    "# Convert date format"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "ca8ff202-2220-445b-922f-867d5b1009b2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# convert sentiment date\n",
    "sentiment['date'] = pd.to_datetime(sentiment['date'])\n",
    "\n",
    "# convert trades timestamp (FIXED)\n",
    "trades['Timestamp IST'] = pd.to_datetime(trades['Timestamp IST'], dayfirst=True)\n",
    "\n",
    "# create date column\n",
    "trades['date'] = trades['Timestamp IST'].dt.date\n",
    "\n",
    "# convert sentiment date to same format\n",
    "sentiment['date'] = sentiment['date'].dt.date\n"
   ]
  },
