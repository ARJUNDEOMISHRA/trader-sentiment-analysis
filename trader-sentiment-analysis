   "id": "ab15f1cb-d889-494b-ba58-6fdc954b2a45",
   "metadata": {},
   "outputs": [],
   "source": [
    "trade_count = merged.groupby('Account').size().reset_index(name='trade_count')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "id": "7c24a884-2f01-4c6f-912b-d7ff8d05a33c",
   "metadata": {},
   "outputs": [],
   "source": [
    "trade_count['Frequency Type'] = trade_count['trade_count'].apply(\n",
    "    lambda x: 'Frequent Trader' if x > trade_count['trade_count'].median()\n",
    "    else 'Infrequent Trader'\n",
    ")\n"
   ]
  },
  {
